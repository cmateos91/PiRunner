{
  "version": 2,
  "redirects": [
    {
      "source": "https://www.runnerpi.xyz/(.*)",
      "destination": "https://runnerpi.xyz/$1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/validation-key.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain; charset=utf-8"
        }
      ]
    }
  ]
}